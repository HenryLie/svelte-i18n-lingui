{
	"name": "svelte-i18n-lingui",
	"version": "0.2.1",
	"author": "Henry Roes Lie",
	"license": "MIT",
	"description": "Add i18n to Svelte/Sveltekit projects using Lingui, using message as the catalog id",
	"repository": "github:HenryLie/svelte-i18n-lingui",
	"keywords": [
		"svelte",
		"sveltekit",
		"i18n",
		"l10n",
		"lingui",
		"multilingual",
		"translation",
		"internationalization",
		"localization",
		"gettext"
	],
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"extract": "lingui extract",
		"compile": "lingui compile --typescript"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./extractor": {
			"types": "./dist/extractor.d.ts",
			"default": "./dist/extractor.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"extractor": [
				"./dist/extractor.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@lingui/core": "^4.5.0",
		"svelte": ">=4"
	},
	"devDependencies": {
		"@lingui/cli": "^5.2.0",
		"@lingui/conf": "^5.2.0",
		"@lingui/core": "^5.2.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/svelte": "^5.2.4",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.45.1",
		"jsdom": "^22.1.0",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.5.0",
		"typescript-svelte-plugin": "^0.3.42",
		"vite": "^5.4.4",
		"vitest": "^1.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^8.12.2",
		"estree-walker-ts": "^1.0.1",
		"js-sha256": "^0.10.1"
	},
	"packageManager": "pnpm@10.0.0+sha256.43abf6e720fb7bc53c591b0862605c7d323d0a9d2dd33bcac07b062e13cf4948",
	"pnpm.onlyBuiltDependencies": [
		"@sveltejs/kit"
	]
}
