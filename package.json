{
	"name": "svelte-i18n-lingui",
	"version": "0.2.0-next.0",
	"author": "Henry Roes Lie",
	"license": "MIT",
	"description": "Add i18n to Svelte/Sveltekit projects using Lingui, using message as the catalog id",
	"repository": "github:HenryLie/svelte-i18n-lingui",
	"keywords": [
		"svelte",
		"sveltekit",
		"i18n",
		"l10n",
		"lingui",
		"multilingual",
		"translation",
		"internationalization",
		"localization"
	],
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"extract": "lingui extract",
		"compile": "lingui compile --typescript"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./extractor": {
			"types": "./dist/extractor.d.ts",
			"default": "./dist/extractor.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"extractor": [
				"./dist/extractor.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@lingui/core": "^4.5.0",
		"svelte": "4.x"
	},
	"devDependencies": {
		"@lingui/cli": "^4.5.0",
		"@lingui/conf": "^4.5.0",
		"@lingui/core": "^4.5.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.26.0",
		"@sveltejs/package": "^2.2.2",
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"@testing-library/svelte": "^4.0.4",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"jsdom": "^22.1.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"publint": "^0.2.5",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"typescript-svelte-plugin": "^0.3.34",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^6.8.0",
		"js-sha256": "^0.10.1",
		"svelte-preprocess": "^5.0.4"
	}
}
